{
    "compilerOptions": {
        "target": "ES2020",
        "lib": ["ES2020"],
        "module": "CommonJS",
        "noEmit": true,
        "strict": false,
        "skipLibCheck": true,
        "isolatedModules": true,
        "resolveJsonModule": true,
        "useDefineForClassFields": true,
        "moduleResolution": "Node",
        "allowJs": true,
        "checkJs": true
    }
}
